<?xml version="1.0" encoding="us-ascii" standalone="yes"?>
<!-- Epa 2012 D 44 issue 5-->
<!--
If ptype is not set or no there then paper id is set o zero
ptype1   = Guest editorial
ptype2   = Commentary
ptype3   = Reviews
ptype4   = Referees
ptype5   = Editorial
ptype6   = Obituary
ptype7   = Author/Subject/Book reviews index
ptype10  = Last but not least
ptype16  = Short and Sweet IPerception
html_in  = directory with html selector of all the indesign html files
xslt_ref = location of reference conversion rules to make html for web
-->
<config>
 <info> 
  <year>2012</year>
  <journal>I</journal>
  <volume>3</volume>
  <issue>7</issue>
  <html_in>../iper/*.html</html_in>
  <save_refs></save_refs>
  <save_db>./iper_db_proofs/</save_db>
  <xslt_ref>./make_refs.xsl</xslt_ref>
  <xslt_title>./make_title.xsl</xslt_title>
  <xslt_abs>./make_abstract.xsl</xslt_abs>
  <xslt_author>./make_author.xsl</xslt_author>
 </info>

 <papers>

  <paper pid='i0495'>
    <start>398</start>
    <finish>409</finish>
    <misc></misc>
    <ptype>16</ptype>
  </paper>

 </papers>
</config>
  


